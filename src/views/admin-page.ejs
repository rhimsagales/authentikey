                                    <button class="btn-circle btn-sm btn-ghost flex items-center justify-center absolute top-2 right-2" popovertarget="delete-modal-container" popovertargetaction="hide">
                                        <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5"
                                        stroke="currentColor" class="size-4">
                                            <path stroke-linecap="round" stroke-linejoin="round" d="M6 18L18 6M6 6l12 12" />
                                        </svg>
                                    </button>
                                    <h2 class="text-lg font-bold mt-2">Delete Admin Account</h2>
                                    <form class="flex flex-col gap-4 items-center justify-center h-full delete-admin-form">
                                        <div class="form-control w-full">
                                            <label class="label" for="delete-username">
                                                <span class="label-text font-medium">Username to Delete</span>
                                            </label>
                                            <input type="text" id="delete-username" class="input input-bordered w-full text-xs" placeholder="Enter username" required>
                                        </div>
                                        <button type="submit" class="btn btn-md  bg-white text-base-100 hover:bg-white/60 w-full mt-2 ease-in-out duration-500" disabled>
                                            Delete Account
                                        </button>
                                    </form>
                                </div>
                            </div>
                        </div>

                        <button class="btn-sm btn-outline rounded-full text-xs px-5 text-base-content border-[1px] border-base-content/20 ease-in-out duration-500 font-semibold" popovertarget="add-modal-container" popovertargetaction="show" >Add</button>
                        <div class="w-screen h-svh bg-base-100/60 backdrop-blur-sm" id="add-modal-container" popover>
                            <div class="w-full h-full flex items-center justify-center">
                                <div class="w-[90%] max-w-xl h-auto min-h-[400px] bg-base-300 relative pt-11 px-2 pb-2 lg:px-4 lg:pb-4">
                                    <button class="btn-circle btn-sm btn-ghost flex items-center justify-center absolute top-2 right-2" popovertarget="add-modal-container" popovertargetaction="hide">
                                        <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5"
                                        stroke="currentColor" class="size-4">
                                            <path stroke-linecap="round" stroke-linejoin="round" d="M6 18L18 6M6 6l12 12" />
                                        </svg>
                                    </button>
                                    <h2 class="text-lg font-bold mt-2">Add Admin Account</h2>
                                    <form class="flex flex-col gap-4 items-center justify-center h-full add-admin-form">
                                        <!-- Username -->
                                        <div class="form-control w-full">
                                            <label class="label" for="add-username">
                                                <span class="label-text font-medium">Username</span>
                                            </label>
                                            <input type="text" id="add-username" class="input input-bordered w-full text-xs" placeholder="Enter username" required>
                                        </div>
                                        <!-- Password -->
                                        <div class="form-control w-full relative">
                                            <label class="label" for="add-password">
                                                <span class="label-text font-medium">Password</span>
                                            </label>
                                            <input type="text" id="add-password" class="input input-bordered w-full text-xs pr-10" placeholder="Enter password" required>
                                            
                                        </div>
                                        
                                        <div class="form-control w-full">
                                            <label class="label" for="edit-new-role">
                                                <span class="label-text font-medium">New Role</span>
                                            </label>
                                            <select id="add-role" class="select select-bordered w-full text-xs">
                                                <option class="bg-base-300" value="" disabled selected>Select new role</option>
                                                <option class="bg-base-300" value="Super Admin">Super Admin (All Permissions)</option>
                                                <option class="bg-base-300" value="Moderator">Moderator (Requests)</option>
                                                <option class="bg-base-300" value="Viewer">Viewer (Logs & Analytics)</option>
                                            </select>
                                        </div>
                                        <button type="submit" class="btn btn-md btn-primary bg-white text-base-100 hover:bg-white/60 w-full mt-2 ease-in-out duration-500" disabled>
                                            Add Account
                                        </button>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="w-full overflow-y-auto"> 
                        <table class="w-full border-collapse">
                        <caption class="w-full h-12 text-sm font-bold text-white text-center bg-base-200 border-[1px] border-base-content/10 rounded-tl-md rounded-tr-md py-[14px] sticky top-0 left-0 right-0">Admin Credentials</caption>
                            <thead class="w-full h-10">
                                <tr class="w-full h-full text-xs font-semibold text-base-content text-center bg-base-200/50">
                                    <th class="min-w-1/4 max-w-[25%] border-[1px] border-base-content/10">
                                        Username
                                    </th>
                                    <th class="min-w-1/4 max-w-[25%] border-[1px] border-base-content/10">
                                        Password
                                    </th>
                                    <th class="min-w-1/4 max-w-[25%] border-[1px] border-base-content/10">
                                        Role
                                    </th>
                                    <th class="min-w-1/4 max-w-[25%] border-[1px] border-base-content/10">
                                        Show/Hide
                                    </th>
                                </tr>
                            </thead>
                            <tbody class="">
                                <% let counter = 1 %>
                                <% let idUsername = `adminUsername${counter}`; %>
                                <% let idPassword = `adminPassword${counter}`; %>
                                <% let idRole = `adminRole${counter}`; %>
                                <% for(const key in adminCredentials){%>
                                    <% const admin = adminCredentials[key] %>
                                    <tr class="text-xs font-normal text-center bg-base-200/20">
                                        <td class="h-10 min-w-1/4 max-w-[25%] border-[1px] border-base-content/10 overflow-x-auto">
                                            <input class="w-full text-center bg-transparent admin-input-username" type="text" name="" id="<%= idUsername%>" value="<%= admin.username%>" disabled>
                                        </td>
                                        <td class="h-10 min-w-1/4 max-w-[25%] border-[1px] border-base-content/10 overflow-x-auto">
                                            <input class="w-full text-center bg-transparent admin-input-password" type="password" name="" id="<%= idPassword%>" value="<%= admin.password%>" disabled>
                                        </td>
                                        <td class="h-10 min-w-1/4 max-w-[25%] border-[1px] border-base-content/10">
                                            <input class="w-full text-center bg-transparent  overflow-x-auto admin-input-permission" type="password" name="" id="<%= idRole%>" value="<%= admin.role%>" disabled>
                                        </td>
                                        <td class="h-10 min-w-[15%] max-w-[15%] border-[1px] border-base-content/10">
                                            <div class="w-full flex items-center justify-center">
                                                <button class="btn-xs btn-circle btn-ghost flex items-center justify-center text-base-content admin-table-show-hide-btns">
                                                    <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-4 block">
                                                        <path stroke-linecap="round" stroke-linejoin="round" d="M2.036 12.322a1.012 1.012 0 0 1 0-.639C3.423 7.51 7.36 4.5 12 4.5c4.638 0 8.573 3.007 9.963 7.178.07.207.07.431 0 .639C20.577 16.49 16.64 19.5 12 19.5c-4.638 0-8.573-3.007-9.963-7.178Z" />
                                                        <path stroke-linecap="round" stroke-linejoin="round" d="M15 12a3 3 0 1 1-6 0 3 3 0 0 1 6 0Z" />
                                                    </svg>
                                                    <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-4 hidden">
                                                        <path stroke-linecap="round" stroke-linejoin="round" d="M3.98 8.223A10.477 10.477 0 0 0 1.934 12C3.226 16.338 7.244 19.5 12 19.5c.993 0 1.953-.138 2.863-.395M6.228 6.228A10.451 10.451 0 0 1 12 4.5c4.756 0 8.773 3.162 10.065 7.498a10.522 10.522 0 0 1-4.293 5.774M6.228 6.228 3 3m3.228 3.228 3.65 3.65m7.894 7.894L21 21m-3.228-3.228-3.65-3.65m0 0a3 3 0 1 0-4.243-4.243m4.242 4.242L9.88 9.88" />
                                                    </svg>
                                                </button>
                                            </div>
                                        </td>
                                    </tr>
                                    <% counter += 1; %>
                                    <% idUsername = `adminUsername${counter}`; %>
                                    <% idPassword = `adminPassword${counter}`; %>
                                    <% idRole = `adminRole${counter}`; %>
                                <% } %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    <% } %>
    
    <!-- Scripts -->
    <script>
        
    </script>

    <script src="../js/admin-tour.js"></script>

    <!-- For sending email -->
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/@emailjs/browser@4/dist/email.min.js">
    </script>

    <script type="text/javascript">
        (async function () {
            const responseFetch = await fetch('/pages/sign-in/change-pass/send/secret');

            const secrets = await responseFetch.json();
            emailjs.init({
            publicKey: secrets.PublicID,
            });
        })();
    </script>
    <script src="../js/inject-data-admin-page.js" type="module"></script>
</body>

</html>