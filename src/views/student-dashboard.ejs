                class="totalLogin flex flex-col justify-between rounded-lg border border-gray-100 bg-white px-2 py-4  sm:p-4  w-full h-full shadow-md">
                <% let thisWeekLogins=loginThisWeek; let lastWeekLogins=loginLastWeek; let change=thisWeekLogins -
                    lastWeekLogins; let percentageChange=lastWeekLogins> 0 ? (change / lastWeekLogins) * 100 : 100;
                    let isIncrease = change > 0;
                    let isSame = change === 0;
                    let bgColor = isIncrease ? 'bg-green-100  text-green-600 '
                    : (isSame ? 'bg-gray-100  text-gray-600 '
                    : 'bg-red-100  text-red-600 ');
                    let iconPath = isIncrease ? "M13 7h8m0 0v8m0-8l-8 8-4-4-6 6"
                    : (isSame ? "M4 12h16"
                    : "M13 17h8m0 0v-8m0 8l-8-8-4 4-6-6");
