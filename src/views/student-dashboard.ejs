    <script type="module">
        
        function getMonths() {
            const months = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
            let currentMonthIndex = new Date().getMonth();

            return [months[(currentMonthIndex + 10) % 12], months[(currentMonthIndex + 11) % 12], months[currentMonthIndex]];
        }
        const canvas = document.getElementById("myChart");

        const ctx = canvas.getContext("2d");

        new Chart(ctx, {
            type: "bar",
            data: {
                labels: getMonths(),
                datasets: [{
                    label: "Total Computer Usage per Month",
                    data: JSON.parse('<%= JSON.stringify(lastThreeMonthsLogins) %>'),
                    backgroundColor: ["#1dcdbc", "#f000b8", "#570df8"]
                }]
            },
            options: {
                plugins: {
                    legend: {
                        labels: {
                            boxWidth: 0
                        }
                    }
                }
            }
        });


        document.addEventListener('DOMContentLoaded', () => {
            const qrContainer = document.querySelector(".qr-code");
            const qr = new QRCode(qrContainer, {
                text: '<%= studentID%>',
                width: 208,
                height: 208
            });

            
            
     


            

            const logsTable = document.querySelector('.logs-table');
            const exportExcelBtn = document.querySelector('.export-excel-btn');

            exportExcelBtn.addEventListener('click', (event) => {
                event.stopPropagation();
                event.preventDefault();

                let wb = XLSX.utils.book_new(); 
                let ws = XLSX.utils.table_to_sheet(logsTable); 

                XLSX.utils.book_append_sheet(wb, ws, "Computer Logs"); 
                XLSX.writeFile(wb, "computer_logs.xlsx"); 
            })

        });
    


    </script>

    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/@emailjs/browser@4/dist/email.min.js">
    </script>

    <script type="text/javascript">
        (async function () {
            const responseFetch = await fetch('/pages/sign-in/change-pass/send/secret');

            const secrets = await responseFetch.json();
            emailjs.init({
                publicKey: secrets.PublicID,
            });
        })();
    </script>

