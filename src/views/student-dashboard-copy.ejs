  <script>
    const myAccountElement = document.querySelector('#my-account-two');

    // Function to handle the modal toggle logic
    const toggleModalIfNeeded = () => {
      if (window.innerHeight <= 590 && myAccountElement.classList.contains("open-modal")) {
        console.log("Viewport height is 590px or less, hiding modal.");
        myAccountElement.click();
      } else if (window.innerHeight > 590 && !myAccountElement.classList.contains("open-modal")) {
        console.log("Viewport height is greater than 590px, showing modal.");
        myAccountElement.click();
      }
    };
    const mediaQuery = window.matchMedia("(max-height: 590px)");

    mediaQuery.addEventListener("change", (e) => {
      toggleModalIfNeeded();
    });

    // Initial check on DOM load
    document.addEventListener("DOMContentLoaded", () => {
      toggleModalIfNeeded();
    });


  </script>

