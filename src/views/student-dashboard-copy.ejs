  <!-- For qr generation -->
  <script>
    window.course = "<%= course%>";
    window.yearLevel = "<%= yearLevel%>";
    let yearLevelNum = parseInt(window.yearLevel[0]);

    function yearLevelDots(yearLevel) {
      let dots = "";
      for (let i = 0; i < yearLevel; ++i) {
        dots += "<div class=\"dot\"></div>"
      }
      return dots;
    }

    window.levelDots = yearLevelDots(yearLevelNum);
    window.svg;
    window.studentID = "<%= studentID %>";



    window.name = "<%= name%>";
    window.email = "<%= email%>";
    async function fetchSvgQRCode(data) {
      await fetch('/generate-qr', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json'
        },
        body: JSON.stringify({
          text: data
        })
      })
        .then(res => res.json())
        .then(data => {
          window.svg = data.svg;
          const qrContainer = document.querySelector(".qr-code").innerHTML = data.svg;
        });
    }
    fetchSvgQRCode(window.studentID)
  </script>


